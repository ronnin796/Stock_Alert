    "import pandas as pd\n",
    "\n",
    "def load_data():\n",
    "    df = pd.read_excel('superstore_sales.xls')\n",
    "    df = df[df['Category'] == 'Technology']\n",
    "    df = df.groupby(['Order Date', 'Sub-Category'])['Quantity'].sum().reset_index()\n",
    "    return df\n",
    "\n",
    "data = load_data()\n",
    "data.head()"
   ]
